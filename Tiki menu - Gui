--Rota peca

Tabs.Rota:AddButton({ Title = "Automático", Callback = function() 
		local A = Vector3.new(-1244.7047119140625, 82.06792449951172, -504.80419921875)
		local B = Vector3.new(-1205.095458984375, 68.3414306640625, -523.4365844726562)
		local C = Vector3.new(-1118.97802734375, 33.28020477294922, -512.0048217773438)
		local Fac = Vector3.new(31.90519142150879, 32.107463836669922, 174.8002166748047)
		local car = workspace:FindFirstChild(NomePlayer)
			game.Players[NomePlayer].PlayerGui.ShopTools.all.pecaLEGAL.Position = UDim2.new(0.5, 0, 0.250000048, 0)
			game.Players[NomePlayer].PlayerGui.ShopTools.all.pecaLEGAL.BackgroundTransparency =	0.75		

		--Não mexer mais nada
			local function teleportCar()
				if car and car then
		--0
			car:SetPrimaryPartCFrame(CFrame.new(A))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(B))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(C))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(Fac))
			game.Players[NomePlayer].PlayerGui.ShopTools.all.pecaLEGAL.coisas.Missao.TextButton.Visible = true
			wait("2")
			game.Players[NomePlayer].PlayerGui.ShopTools.all.pecaLEGAL.coisas.Missao.TextButton.Visible = false
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(A))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(B))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(C))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(Fac))
			game.Players[NomePlayer].PlayerGui.ShopTools.all.pecaLEGAL.coisas.Missao.TextButton.Visible = true
			wait("2")
			game.Players[NomePlayer].PlayerGui.ShopTools.all.pecaLEGAL.coisas.Missao.TextButton.Visible = false
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(A))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(B))
		--10
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(C))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(Fac))
			game.Players[NomePlayer].PlayerGui.ShopTools.all.pecaLEGAL.coisas.Missao.TextButton.Visible = true
			wait("2")
			game.Players[NomePlayer].PlayerGui.ShopTools.all.pecaLEGAL.coisas.Missao.TextButton.Visible = false
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(A))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(B))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(C))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(Fac))
			game.Players[NomePlayer].PlayerGui.ShopTools.all.pecaLEGAL.coisas.Missao.TextButton.Visible = true
			wait("2")
			game.Players[NomePlayer].PlayerGui.ShopTools.all.pecaLEGAL.coisas.Missao.TextButton.Visible = false
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(A))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(B))
			wait("1")
			car:SetPrimaryPartCFrame(CFrame.new(C))
		--20

				else
					print("Chora para o markito")
				end
			end
	teleportCar()
end })

Tabs.Rota:AddButton({ Title = "Auto", Callback = function() 
