local tempo = "16"
local NomePlayer = "pg_nicolas3"

--Carregar Blioteca
	local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

--Criar Janela
	local Window = Fluent:CreateWindow({
	Title = "Tiki Menu - BETA " .. Fluent.Version,
	SubTitle = "by nicolas025",
	TabWidth = 160, Size = UDim2.fromOffset(470, 350), Theme = "Dark", MinimizeKey = Enum.KeyCode.LeftAlt
	})

--Abas
	local Tabs = {
	Farm = Window:AddTab({ Title = "Farm eva" }),
	Rota = Window:AddTab({ Title = "Farm peca" }),
	Teleporte = Window:AddTab({ Title = "Teleporte" }),
	Frames = Window:AddTab({ Title = "Frames" })
	}	

--Teleporte

Tabs.Teleporte:AddButton({ Title = "Garagem", Callback = function() 

	local car = workspace:FindFirstChild(NomePlayer)

	--Não mexer mais nada
	local function teleportCar()
		if car and car then
	car:SetPrimaryPartCFrame(CFrame.new(-484.2584533691406, 5.273308277130127, 357.2752380371094))
		else
			print("Chora para o markito")
		end
	end
	teleportCar()
end })

