			car:SetPrimaryPartCFrame(CFrame.new(targetPosition2))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition3))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition4))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition2))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition3))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition4))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition2))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition3))
			wait(tempo)
			--180
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition2))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition3))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition4))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition2))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition3))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition4))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition2))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition3))
			wait(tempo)
			--190
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition2))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition3))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition4))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition2))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition3))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition4))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition2))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition3))
			wait(tempo)
			--200
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition2))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition3))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition4))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition2))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition3))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition4))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition2))
			wait(tempo)
			car:SetPrimaryPartCFrame(CFrame.new(targetPosition3))		
		else
			print("Chora para o markito")
		end
	end
	teleportCar()
end })






