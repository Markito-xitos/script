Tabs.Farm:AddButton({ Title = "Farmar - 100 plantas", Callback = function() 

	local car = workspace:FindFirstChild(NomePlayer)
	local targetPosition = Vector3.new(12045.12109375, 27.670997619628906, 12796.1669921875)
	local targetPosition2 = Vector3.new(12037.2705078125, 27.670982360839844, 12794.0634765625)
	local targetPosition3 = Vector3.new(12030.3427734375, 27.67096710205078, 12792.20703125)
	local targetPosition4 = Vector3.new(12022.83203125, 27.67095184326172, 12790.1943359375)

	--Não mexer mais nada
	local function teleportCar()
